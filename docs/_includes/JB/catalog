{% if site.JB.catalog and site.JB.catalog != '' %}
    {% assign CATALOG = "" | split: "" %}
    {% for category in site.JB.catalog %}
        {% for cate in site.categories %}
            {% if cate[0] == category %}
                {% assign CATALOG = CATALOG | push:cate %}
            {% endif %}
        {% endfor %}
    {% endfor %}
{% else %}
    {% assign CATALOG = site.categories %}
{% endif %}